{
  "name": "djq-node-ts",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js",
    "dev:producer": "ts-node-dev --respawn src/examples/producer.ts",
    "dev:worker": "ts-node-dev --respawn src/examples/producer.ts",
    "dev": "concurrently \"ts-node-dev --respawn src/examples/producer.ts\" \"ts-node-dev --respawn src/examples/basicWorkerProcess.ts\""
  },
  "dependencies": {
    "redis": "^4.6.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
